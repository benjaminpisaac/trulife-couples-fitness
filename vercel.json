{
    "version": 2,
    "builds": [
        {
            "src": "TruLife.API/TruLife.API.csproj",
            "use": "@vercel/dotnet"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "TruLife.API/TruLife.API.csproj"
        }
    ],
    "env": {
        "GEMINI_API_KEY": "@gemini_api_key",
        "JWT_SECRET": "@jwt_secret",
        "JWT_KEY": "@jwt_secret",
        "JWT_ISSUER": "TruLifeAPI",
        "JWT_AUDIENCE": "TruLifeClient"
    }
}