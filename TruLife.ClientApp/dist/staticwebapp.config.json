{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "platform": {
        "apiRuntime": "dotnet:8.0"
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;"
    }
}