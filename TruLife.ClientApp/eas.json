{
    "cli": {
        "version": ">= 5.9.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": false,
                "buildConfiguration": "Debug"
            },
            "env": {
                "API_URL": "http://localhost:5000"
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": false
            },
            "env": {
                "API_URL": "https://trulife-couples-fitness.onrender.com"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "API_URL": "https://trulife-couples-fitness.onrender.com"
            }
        }
    }
}