{
    "appId": "com.trulife.fitness",
    "appName": "TruLife Couples Fitness",
    "webDir": "dist",
    "server": {
        "androidScheme": "https"
    },
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 2000,
            "backgroundColor": "#6366f1",
            "showSpinner": true,
            "spinnerColor": "#ffffff"
        },
        "PushNotifications": {
            "presentationOptions": [
                "badge",
                "sound",
                "alert"
            ]
        }
    }
}